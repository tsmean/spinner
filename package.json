    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^2.2.0",
